(include "vbar/config.yuck")

(defwindow wallpaper
    :monitor 0
    :geometry (geometry
        :x 0
        :y 0
        :width "100%"
        :height "100%"
        :anchor "center left"
    )
    :stacking "bg"
    (image
        :path {get_env("WALLPAPER")}
        :image-width 1920
    )
)


(defwindow sidebar
    :monitor 0
    :geometry (geometry
        :x "32px"
        :y 0
        :height "1016px"
        :width "48px"
        :anchor "center left"
    )
    (vbar)
)

